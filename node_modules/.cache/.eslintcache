n    cursor: pointer;\r\n    display: block;\r\n    position: relative;\r\n    padding: 4px;\r\n\r\n    img {\r\n      width: 100%;\r\n      height: 100%;\r\n    }\r\n\r\n    &:hover {\r\n      padding: 0;\r\n      border: 4px solid rgba(249, 249, 249, 0.8);\r\n      transition-duration: 300ms;\r\n    }\r\n  }\r\n`;\r\n\r\nexport default ImgSlider;\r\n","D:\\Belajar Website\\Belajar ReactJS\\React JS Project\\disney-clone - Copy\\src\\components\\Viewers.js",[],[],"D:\\Belajar Website\\Belajar ReactJS\\React JS Project\\disney-clone - Copy\\src\\components\\Recommends.js",[],[],"D:\\Belajar Website\\Belajar ReactJS\\React JS Project\\disney-clone - Copy\\src\\components\\NewDisney.js",[],[],"D:\\Belajar Website\\Belajar ReactJS\\React JS Project\\disney-clone - Copy\\src\\components\\Originals.js",[],[],"D:\\Belajar Website\\Belajar ReactJS\\React JS Project\\disney-clone - Copy\\src\\features\\movie\\movieSlice.js",[],[],"D:\\Belajar Website\\Belajar ReactJS\\React JS Project\\disney-clone - Copy\\src\\components\\Trending.js",[],[],"D:\\Belajar Website\\Belajar ReactJS\\React JS Project\\disney-clone - Copy\\src\\components\\Detail.js",[],[],"D:\\Belajar Website\\Belajar ReactJS\\React JS Project\\disney-clone - Copy\\src\\components\\LazyLoad.js",[],[],{"ruleId":"100","severity":1,"message":"101","line":65,"column":6,"nodeType":"102","endLine":65,"endColumn":16,"suggestions":"103"},{"ruleId":"104","severity":1,"message":"105","line":24,"column":31,"nodeType":"106","messageId":"107","endLine":24,"endColumn":33},{"ruleId":"100","severity":1,"message":"108","line":27,"column":26,"nodeType":"102","endLine":27,"endColumn":72},{"ruleId":"100","severity":1,"message":"109","line":30,"column":25,"nodeType":"102","endLine":30,"endColumn":70},{"ruleId":"100","severity":1,"message":"110","line":33,"column":25,"nodeType":"102","endLine":33,"endColumn":70},{"ruleId":"100","severity":1,"message":"111","line":36,"column":24,"nodeType":"102","endLine":36,"endColumn":68},{"ruleId"